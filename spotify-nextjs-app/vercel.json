{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    }
  ],
  "buildCommand": "next build",
  "outputDirectory": ".next",
  "functions": {
    "api/**/*": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "env": {
    "NODE_ENV": "production",
    "NEXT_PUBLIC_PIPED_PROXY_URL": "@piped-proxy-url",
    "NEXT_PUBLIC_PIPED_API_URL": "@piped-api-url"
  }
}